{
  "rewrites": [
    {
      "source": "/capsule/:id",
      "destination": "/capsule/index.html"
    },
    {
      "source": "/open/:id",
      "destination": "/capsule/index.html"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
